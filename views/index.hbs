<div class="col-md-6 text-center" id="employees">
    <div class="row">
        <h2>Employees</h2>
    </div>
    {{#each member_data}}
    <div class="row">
        <div class="col-md-6 ">
            <h3>{{this.first_name}}</h3>
        </div>
        <div class="col-md-6 ">
            <form class="employee-projects">
            <input type="hidden" class="member_id" type="text" value={{this.id}}><br>
            <button type="submit" class="btn btn-default">View Projects</button>
            </form>
        </div>
    </div>
    {{/each}}
</div>