<div class="col-md-12" id="tasks">
    <div class="row">
        <h2>{{this.title}}</h2>
    </div>
    <div class="row">
        <div class="col-md-4">
            <h3>Task</h3>
        </div>
        <div class="col-md-4">
            <h3>Assigned To</h3>
        </div>
        <div class="col-md-4">
            <h3>Estimated Hours</h3>
        </div>
    </div>
    {{#each task_data}}
    <div class="row">
        <div class="col-md-4 ">
            <h3>{{this.task}}</h3>
        </div>
        <div class="col-md-4 ">
            <h3>{{this.member}}</h3>
        </div>
        <div class="col-md-4 ">
            <h3>{{this.hours}}</h3>
        </div>
    </div>
    {{/each}}
</div>